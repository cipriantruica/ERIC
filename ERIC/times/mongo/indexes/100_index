pos=[4.794, 4.816, 5.042, 4.821, 4.705, 4.881, 4.902, 5.45, 4.986, 5.054]
iv=[6.113, 5.567, 5.942, 5.907, 6.058, 6.246, 5.669, 5.766, 6.041, 6.094]
vocab=[6.786, 6.38, 6.781, 6.866, 6.951, 6.36, 6.214, 6.67, 6.255, 6.467]
print "pos:", round(sum(pos)/len(pos),2)
